{
  "output": "./output",
  "helpers": {
    "Protractor": {
      "url": "https://demo.virtocommerce.com/",
      "driver": "hosted",
      "browser": "chrome",
      "seleniumAddress": "http://localhost:4444/wd/hub",
      "angular": "true",
      "rootElement": "body",
      "fullPageScreenshots": "true"
    },
    "REST": {
      "endpoint": "http://demo.virtocommerce.com/admin/api",
      "defaultHeaders": {
        "auth": "a348fa7508d342f6a32f8bf6c6681a2a"
      }
    },
    "ApiDataFactory": {
      "endpoint": "http://demo.virtocommerce.com/admin/api",
      "cleanup": true,
      "headers": {
        "user-key": "a348fa7508d342f6a32f8bf6c6681a2a",
        "Content-Type": "application/json",
        "Accept": "application/json"
      },
      "factories": {
        "user": {
          "factory": "./factories/users",
          "create": { "user": "/platform/security/users/create" },
          "delete": { "user": "/platform/security/users" }
        }
      }
    }
  },
  "include": {
    "I": "./steps_file.js",
    "createAccountPage": "./pages/createAccount.js",
    "navigationPage": "./pages/navigation.js",
    "headerPage": "./pages/header.js",
    "productListPage": "./pages/productList.js",
    "addProduct": "./pages/addProduct.js",
    "mergingCart": "./pages/mergingCart.js",
    "modalDialogFragment": "./fragments/modalDialog.js",
    "cartItemCountFragment": "./fragments/cartItemCount.js",
    "shippingAdressFragment": "./fragments/shippingAdress.js",
    "orderSummaryFragment": "./fragments/orderSummary.js",
    "search": "./pages/search.js",
    "productCompare": "./pages/productCompare.js",
    "storeOptions": "./pages/storeOptions.js",
    "newSubscription": "./pages/newSubscription.js",
    "bulkOrder": "./pages/bulkOrder.js",
    "blogs": "./pages/blogs.js",
    "createAddressPage": "./pages/createAddress.js"
  },
  "mocha": {},
  "bootstrap": false,
  "teardown": null,
  "hooks": [],
  "gherkin": {},
  "plugins": {
    "screenshotOnFail": {
      "enabled": true
    },
    "allure": {
      "enabled": true
    }
  },
  "tests": "./tests/*_test.js",
  "timeout": 10000,
  "name": "QualityGate"
}
